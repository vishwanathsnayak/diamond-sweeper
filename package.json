{
  "name": "diamond-sweeper",
  "version": "1.0.0",
  "description": "Diamond Sweeper",
  "scripts": {
    "prebuild": "npm run lint",
    "build": "npm run clean && webpack -p --config=webpack.prod.config.babel.js",
    "clean": "rimraf dist/*",
    "dev": "webpack-dev-server --colors --open --watch --hot --inline",
    "lint": "esw webpack.config.* src --color",
    "lint:watch": "npm run lint -- --watch",
    "start": "npm-run-all --parallel dev lint:watch",
    "test": "karma start"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "webpack",
    "karma",
    "mocha",
    "chai",
    "enzyme",
    "PhantomJS"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vishwanathsnayak/diamond-sweeper.git"
  },
  "private": true,
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "normalize.css": "^7.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "css-loader": "^0.28.7",
    "enzyme": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-watch": "^3.1.2",
    "extract-text-webpack-plugin": "^2.1.2",
    "fetch-mock": "^5.12.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.7",
    "karma": "^1.7.1",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "mocha": "^3.5.3",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "prop-types": "^15.6.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^15.6.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.8.2"
  }
}
